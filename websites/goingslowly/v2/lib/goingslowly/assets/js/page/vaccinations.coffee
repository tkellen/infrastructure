window.vac = (data) ->
  i = 0
  output = []
  output.push "<table id=\"vactable\" width=100%>"
  output.push "  <thead>"
  output.push "    <tr>"
  output.push "      <th>Country</th>"
  output.push "      <th class='col0'>Hep&nbsp;A</th>"
  output.push "      <th class='col1'>Typhoid</th>"
  output.push "      <th class='col2'>Tetanus<br/>Diphtheria<br/>Pertussus</th>"
  output.push "      <th class='col3'>Hep&nbsp;B</th>"
  output.push "      <th class='col4'>Meningitis</th>"
  output.push "      <th class='col5'>Rabies</th>"
  output.push "      <th class='col6'>Yellow<br/>Fever</th>"
  output.push "      <th class='col7'>Jap.<br/>Enceph.</th>"
  output.push "      <th class='col8'>Polio</th>"
  output.push "      <th class='col9'>MMR</th>"
  output.push "    </tr>"
  output.push "  </thead>"
  output.push "  <tbody id=\"vacinfo\">"
  for a of data.area
    output.push "    <tr class='area'><td class=\"area\" colspan=11>" + data.areas[i] + "</td></tr>"
    for c of data.area[a]
      output.push "    <tr>"
      output.push "      <td class=\"country\">" + c + "</td>"
      for d of data.area[a][c]
        continue  if d is "remove"
        col = (if data.area[a][c][d] is " " then "n" else data.area[a][c][d])
        content = (if col is "n" then "-" else col)
        output.push "      <td class='col" + d + "'>" + content + "</td>"
      output.push "    </tr>"
    i++
  output.push "  </tbody>"
  output.push "</table>"
  output.join "\n"
